{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-plusplus": "off",
        "no-underscore-dangle": "off",
        "no-continue": "off",
        "consistent-return": "off"
    }
}
